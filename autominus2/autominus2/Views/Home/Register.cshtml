@{
    ViewData["Title"] = "Registracija";
    autominus2.Utils.OurSession.InRegistration = true;
}


<div class="form-group text-center">
    <form method="POST" action="Login">

        <div class="col-md-10">
            <th>Vardas</th>
            <br />
            <input type="text" name="registerNameInput" required/>
        </div>

        <div class="col-md-10">
            <th>Pavarde</th>
            <br />
            <input type="text" name="registerLastNameInput" required/>
        </div>

        <div class="col-md-10">
            <th>Slapyvardis</th>
            <br />
            <input type="text" name="registerUserNameInput" required/>
        </div>

        <div class="col-md-10">
            <th>Miestas</th>
            <br />
            <input type="text" name="registerCityInput" required/>
        </div>

        <div class="col-md-10">
            <th>Gimimo metai</th>
            <br />
            <input type="date" name="registerBirthDateInput" required/>
        </div>

        <div class="col-md-10">
            <th>Telefono numeris</th>
            <br />
            <input type="text" name="registerPhoneNumberInput" required/>
        </div>

        <div class="col-md-10">
            <th>E-Pašto adresas</th>
            <br />
            <input type="email" name="registerEmailInput" required/>
        </div>

        <div class="col-md-10">
            <th>Slaptažodis</th>
            <br />
            <input type="password" name="registerPasswordInput" required/>
        </div>

        <div class="col-md-10">
            <th>Slaptažodžio patvirtinimas</th>
            <br />
            <input type="password" name="registerPasswordConfirmInput" required/>
        </div>



        <div class="col-md-10">
            <br />

            <form>
                <button>Registruotis</button>
            </form>

        </div>

    </form>

</div>
<br />